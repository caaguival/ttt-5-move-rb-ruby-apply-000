#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

board= [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Welcome to Tic Tac Toe!"

puts display_board

puts "Where would you like to go? Please choose a number 1-9 from top left to bottom right."
value="X"
input=gets.strip
index=input_to_index(input)
move board, index, value
display_board(board)